{
  "agent_name": "Customer Service Agent Template",
  "voice_id": "11labs-Adrian",
  "voice_speed": 1.0,
  "voice_temperature": 0.8,
  "interruption_sensitivity": 0.5,
  "responsiveness": 0.8,
  "language": "en-US",
  "enable_backchannel": true,
  "backchannel_frequency": 0.8,
  "ambient_sound": "office",
  "boosted_keywords": ["support", "help", "issue", "refund"],
  "normalize_for_speech": true,
  "llm_config": {
    "model": "gpt-4o-mini",
    "temperature": 0.7,
    "prompt_config": {
      "sections": [
        "base/greeting",
        "base/tone-professional",
        "customer-service/order-lookup",
        "customer-service/refund-policy",
        "base/closing"
      ],
      "variables": {
        "company_name": "Your Company",
        "support_hours": "9am-5pm"
      },
      "dynamic_variables": {
        "customer_name": {
          "type": "string",
          "description": "The customer's full name"
        },
        "order_id": {
          "type": "string",
          "description": "Customer's order ID"
        }
      }
    },
    "begin_message": "Hello! How can I help you today?"
  }
}
